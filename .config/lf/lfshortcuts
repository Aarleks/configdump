# vim:filetype=vim
map ghome cd ~/
map Mhome $(mv $f ~/)
map Chome $(cp $f ~/)
map gconf cd ~/.config
map Mconf $(mv $f ~/.config)
map Cconf $(cp $f ~/.config)
map gscr cd ~/.scripts
map Mscr $(mv $f ~/.scripts)
map Cscr $(cp $f ~/.scripts)
map gdown cd ~/Downloads
map Mdown $(mv $f ~/Downloads)
map Cdown $(cp $f ~/Downloads)
map grd1 cd ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1
map Mrd1 $(mv $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1)
map Crd1 $(cp $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1)
map grd2 cd ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2
map Mrd2 $(mv $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2)
map Crd2 $(cp $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2)
map grlit cd ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit
map Mrlit $(mv $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit)
map Crlit $(cp $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit)
map gcu cd ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units
map Mcu $(mv $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units)
map Ccu $(cp $f ~/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units)
map gcv cd ~/Dropbox/WritingTools/CV
map Mcv $(mv $f ~/Dropbox/WritingTools/CV)
map Ccv $(cp $f ~/Dropbox/WritingTools/CV)
map gproj cd ~/Projects
map Mproj $(mv $f ~/Projects)
map Cproj $(cp $f ~/Projects)
map gadp cd ~/Dropbox/MRes/Development/ADP
map Madp $(mv $f ~/Dropbox/MRes/Development/ADP)
map Cadp $(cp $f ~/Dropbox/MRes/Development/ADP)
map cfa :e ~/.config/aliasrc<CR>
map cfc :e ~/.config/alias-conf<CR>
map cfd :e ~/.config/alias-dirs<CR>
map cfz :e ~/.zshrc<CR>
map cfp :e ~/.profile<CR>
map cfv :e ~/.config/nvim/init.vim<CR>
map cfm :e ~/.config/mutt/muttrc<CR>
map cfi :e ~/.config/i3/config<CR>
map cfnew :e ~/.config/newsboat/urls<CR>
map cfx :e ~/.xinitrc<CR>
map cfxp :e ~/.xprofile<CR>
map cff :e ~/.config/fontconfig/fonts.conf<CR>

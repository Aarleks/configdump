# vim:filetype=vim
map ghome cd /home/alex/
map Mhome $(mv $f /home/alex/)
map Chome $(cp $f /home/alex/)
map gconf cd /home/alex/config
map Mconf $(mv $f /home/alex/config)
map Cconf $(cp $f /home/alex/config)
map gscr cd /home/alex/scripts
map Mscr $(mv $f /home/alex/scripts)
map Cscr $(cp $f /home/alex/scripts)
map gdown cd /home/alex/Downloads
map Mdown $(mv $f /home/alex/Downloads)
map Cdown $(cp $f /home/alex/Downloads)
map grd1 cd /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1
map Mrd1 $(mv $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1)
map Crd1 $(cp $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD1)
map grd2 cd /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2
map Mrd2 $(mv $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2)
map Crd2 $(cp $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RD2)
map grlit cd /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit
map Mrlit $(mv $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit)
map Crlit $(cp $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/RLit)
map gcu cd /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/
map Mcu $(mv $f /home/alex/Dropbox/MRes/mres_shared_folder/1_UNITS/1_Current_Units/)
